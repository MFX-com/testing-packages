{
  "name": "nexera-monorepo",
  "version": "1.0.1",
  "description": "aaa",
  "keywords": [],
  "engines": {
    "node": ">=v18.17.0",
    "pnpm": ">=8.6.10"
  },
  "packageManager": "pnpm@8.6.10",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "release:major": "npm version $(semver $npm_package_version -i major)",
    "release:minor": "npm version $(semver $npm_package_version -i minor)",
    "release:patch": "npm version $(semver $npm_package_version -i patch)",
    "version:major": "standard-version --release-as major",
    "version:minor": "standard-version --release-as minor",
    "version:patch": "standard-version --release-as patch",
    "version": "changeset version",
    "release": "changeset publish",
    "docker:first": "docker build -f packages/first/Dockerfile -t first-app .",
    "docker:second": "docker build -f packages/second/Dockerfile -t second-app .",
    "build:@MFX-org/first": "turbo run build --filter @MFX-org/first",
    "bump:@MFX-org/first": "turbo run bump --filter @MFX-org/first",
    "build:@MFX-org/second": "turbo run build --filter @MFX-org/second",
    "bump:@MFX-org/second": "turbo run bump --filter @MFX-org/second",
    "build:@MFX-org/private": "turbo run build --filter @MFX-org/private",
    "bump:@MFX-org/private": "turbo run bump --filter @MFX-org/private"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "ethers": "^6.7.0",
    "lerna": "^7.1.5",
    "turbo": "^1.10.12"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.405.0",
    "drizzle-orm": "^0.28.6"
  }
}